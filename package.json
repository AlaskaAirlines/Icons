{
  "name": "@alaskaairux/orion-icons",
  "description": "Orion Design System icons",
  "version": "0.1.0",
  "main": "dist/index.js",
  "author": "Alaska Design and Research",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/AlaskaAirlines/OrionIcons"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "auto-changelog": "^1.13.0",
    "concat": "^1.0.3",
    "concurrently": "^4.1.0",
    "copyfiles": "^2.1.0",
    "file-system": "^2.2.2",
    "husky": "^2.1.0",
    "jest": "^24.7.1",
    "jsonlint2": "^1.7.1",
    "semantic-release": "^15.13.12",
    "svglint": "^1.0.2"
  },
  "scripts": {
    "jsonlint": "jsonlint src/data/orion-icons.json -q -V .jsonlintschema",
    "svglint": "svglint src/icons/* --ci",
    "newbuild": "node scripts/build.js",
    "copySass": "copyfiles -f ./src/*.scss ./dist",
    "test": "npm run jsonlint || npm run svglint",
    "svgTest": "jest",
    "surv": "python -m SimpleHTTPServer 9001",
    "browser": "open http://localhost:9001/validate/icons.html",
    "icoserve": "concurrently \"npm run surv\" \"npm run browser\"",
    "changelog": "auto-changelog --template ./scripts/template.hbs --output ./CHANGELOG.md -p -b 5",
    "concat": "concat -o ./README.md ./README.md ./CHANGELOG.md"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release": {
    "branch": "master"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "alaska airlines",
    "orion design system",
    "icons"
  ],
  "contributors": [
    {
      "name": "Dale Sande",
      "email": "dale.sande@alaskaair.com"
    },
    {
      "name": "Camron Lockeby",
      "email": "camron.lockeby@AlaskaAir.com"
    },
    {
      "name": "Kevin Sonnichsen",
      "email": "Kevin.Sonnichsen@alaskaair.com"
    }
  ]
}
