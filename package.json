{
  "name": "@alaskaairux/orion-icons",
  "description": "Orion Design System icons",
  "version": "2.1.0",
  "main": "dist/index.js",
  "author": "Alaska Design and Research",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/AlaskaAirlines/OrionIcons"
  },
  "devDependencies": {
    "@alaskaairux/orion-design-tokens": "^2.10.5",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^7.6.0",
    "@semantic-release/changelog": "^3.0.4",
    "@semantic-release/git": "^7.0.16",
    "@semantic-release/npm": "^5.1.13",
    "concurrently": "^4.1.0",
    "convert-svg-to-png": "^0.5.0",
    "copyfiles": "^2.1.0",
    "file-system": "^2.2.2",
    "husky": "^2.3.0",
    "jest": "^24.8.0",
    "jsonlint2": "^1.7.1",
    "lodash": "^4.17.15",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "semantic-release": "^15.13.21",
    "style-dictionary": "^2.8.1",
    "svglint": "^1.0.4",
    "wc-sass-render": "^1.2.1"
  },
  "scripts": {
    "jsonlint": "jsonlint src/data/icons.json -q -V .jsonlintschema",
    "svglint": "svglint dist/icons/**/*.svg --ci",
    "primarybuild": "node scripts/primarybuild.js",
    "sassRender": "sass-render dist/tokens/*.css",
    "copySass": "copyfiles -f ./src/*.scss ./dist",
    "test": "npm-run-all jsonlint svglint",
    "svgTest": "jest",
    "delete": "rm -rf ./dist",
    "png@2x": "convert-svg-to-png --height 48px dist/icons/*24@2x.png",
    "png@3x": "convert-svg-to-png --height 72px dist/icons/*24@3x.png",
    "ciBuild": "npm-run-all delete primarybuild sassRender png@2x png@3x copySass svgTest",
    "devBuild": "npm-run-all delete primarybuild sassRender copySass",
    "watch": "nodemon -e svg,scss,js --watch src --exec npm run svgTest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release": {
    "branch": "master",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "./CHANGELOG.md",
          "changelogTitle": "# Semantic Release Automated Changelog"
        }
      ],
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "./CHANGELOG.md",
            "package.json"
          ]
        }
      ],
      "@semantic-release/github"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "alaska airlines",
    "orion design system",
    "icons"
  ],
  "contributors": [
    {
      "name": "Dale Sande",
      "email": "dale.sande@alaskaair.com"
    },
    {
      "name": "Camron Lockeby",
      "email": "camron.lockeby@AlaskaAir.com"
    },
    {
      "name": "Kevin Sonnichsen",
      "email": "Kevin.Sonnichsen@alaskaair.com"
    }
  ],
  "dependencies": {
    "fs": "0.0.1-security"
  }
}
